{% extends 'NemesisAdminBundle::admin.html.twig' %}

{% block title %}
    Типы полей :: {{ parent() }}
{% endblock %}

{% block header %}
    <a href="{{ path('storable_forms_field_list') }}">Поля</a> &mdash; Типы полей
{% endblock %}

{% block body %}
    <ul>
        {% for alias, type in types %}
            <li>
                <a href="{{ path('storable_forms_field_create', {'type':alias}) }}">{{ 'storable_field.'~alias |trans }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}

